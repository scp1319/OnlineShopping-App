-------------------------
TBL_ITEM
-------------------------
CREATE TABLE "HR"."TBL_ITEM" 
   (	"ITEM_ID" NUMBER NOT NULL ENABLE, 
	"QUANTITY" NUMBER, 
	"PRODUCT_ID" NUMBER, 
	"CART_ID" NUMBER, 
	 CONSTRAINT "TBL_ITEM_PK" PRIMARY KEY ("ITEM_ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT)
  TABLESPACE "USERS"  ENABLE, 
	 CONSTRAINT "FK_PRODUCT_ID" FOREIGN KEY ("PRODUCT_ID")
	  REFERENCES "HR"."TBL_PRODUCT" ("PRODUCT_ID") ENABLE, 
	 CONSTRAINT "FK_CART_ID_ITEM" FOREIGN KEY ("CART_ID")
	  REFERENCES "HR"."TBL_CART" ("CART_ID") ENABLE
   ) PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT)
  TABLESPACE "USERS" ;

-------------------------
TBL_PRODUCT
-------------------------
CREATE TABLE "HR"."TBL_PRODUCT" 
   (	"PRODUCT_ID" NUMBER NOT NULL ENABLE, 
	"NAME" VARCHAR2(20 BYTE), 
	"BRAND" VARCHAR2(20 BYTE), 
	"PRICE" NUMBER, 
	"CATG_ID" NUMBER, 
	"IMG_SRC" VARCHAR2(100 BYTE), 
	"STOCK" NUMBER, 
	"DESCRIPTION" VARCHAR2(400 BYTE), 
	"RETAILER_ID" NUMBER, 
	 CONSTRAINT "TBL_PRODUCT_PK" PRIMARY KEY ("PRODUCT_ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT)
  TABLESPACE "USERS"  ENABLE, 
	 CONSTRAINT "FK_CATG_ID" FOREIGN KEY ("CATG_ID")
	  REFERENCES "HR"."TBL_CATEGORY" ("CATG_ID") ENABLE, 
	 CONSTRAINT "RETAILER_FK" FOREIGN KEY ("RETAILER_ID")
	  REFERENCES "HR"."TBL_RETAILER" ("RETAILER_ID") ENABLE
   ) PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT)
  TABLESPACE "USERS" ;


--------------------------
TBL_ORDER
---------------------------
 
 CREATE TABLE "HR"."TBL_ORDER" 
   (	"ORDER_ID" NUMBER NOT NULL ENABLE, 
	"DATETIME" TIMESTAMP (6), 
	"STATUS" VARCHAR2(20 BYTE), 
	"PAYMENT_MODE" VARCHAR2(20 BYTE), 
	"PRODUCT_ID" NUMBER, 
	"USER_ID" NUMBER, 
	 CONSTRAINT "TBL_ORDER_PK" PRIMARY KEY ("ORDER_ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT)
  TABLESPACE "USERS"  ENABLE
   ) PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT)
  TABLESPACE "USERS" ;
